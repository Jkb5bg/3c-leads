# AWS S3 Configuration
# You will need to create an S3 bucket and get AWS credentials
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key_here
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET_NAME=your-bucket-name

# Login Password
# Set your desired password for accessing the CRM
REACT_APP_LOGIN_PASSWORD=your_secure_password_here
